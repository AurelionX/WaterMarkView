<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WaterMarkView">
        <attr name="degree" format="integer|reference" />
        <attr name="text" format="string|reference" />
        <attr name="textColor" format="color|reference" />
        <attr name="textSize" format="dimension|reference" />
        <attr name="textBold" format="boolean" />
        <attr name="dx" format="dimension|reference" />
        <attr name="dy" format="dimension|reference" />
        <attr name="align" format="dimension">
            <enum name="LEFT" value="0" />
            <enum name="CENTER" value="1" />
            <enum name="RIGHT" value="2" />
        </attr>
        <attr name="sync" format="boolean" />
    </declare-styleable>
</resources>